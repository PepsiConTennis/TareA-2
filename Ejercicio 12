#include <stdio.h>
#include <stdlib.h>

void serie()
{

    int i;
    int x=0;
    int n;
    int producto = 1;
    
    printf("Â¿Hasta que cantidad de digitos desea multiplicar?: \n");
    scanf("%d", &n);
    
    for (i=0; i<n; i++)
    {
        x = x + 1;
        producto= producto * x;
    }
    
    printf("el valor del producto es de: %d\n", producto);
}

int main()
{
    serie();
    return 0;
}
