#include <stdio.h>

void lista(int arr[])
{
    for (int i = 0; i < 100; i++)
    {
        arr[i] = (i + 1) * 2;
    }
}

void serie(int arr[])
{
    printf("La lista de los nÃºmeros parws es:\n");
    
    for (int i = 0; i < 100; i++)
    {
        printf("%d ", arr[i]);
    }
    printf("\n");
}

int main()
{
    
    int arr[100];

    lista(arr);
    serie(arr);

    return 0;
}
