#include <stdio.h>
#include <stdlib.h>

int serie(int x){
    
    for ( int i = 1; i <= 10; i++){
        printf("%d * %d = %d\n", x, i, x * i);
    }
    
    return 0;
    
}

int main()
{
    
    int x;
    
    printf("Escriba un numero: \n");
    scanf("%d", &x);
    printf("La tabla de multiplicar de %d es; \n", x);
    serie(x);
    
    return 0;
}
